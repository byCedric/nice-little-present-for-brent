{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        "build/**",
        ".expo/**",
        "public/dist/**"
      ],
      "dependsOn": [
        "^build"
      ]
    },
    "clean": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "cache": false
    }
  }
}
